#include <iostream>
#include <vector>
#include<math.h>
using namespace std;

class Solution {
    
public:
    int attempts(vector<int>& nums, int x, int y, int n) {
        int i, jump=0, total=0;
        for(i=0; i<n; i++){
            jump = ceil((nums[i]-x)/(float)(x-y)) +1;
            total+=jump;
        }
        return total;
    }
};

int main() {
    Solution solution;
    vector<int> nums = {11, 34, 27, 9};  
    
    int ans = solution.attempts(nums,10,1,4);
    
    cout << "Number of jumps: " << ans<< endl;
    
    return 0;
}

